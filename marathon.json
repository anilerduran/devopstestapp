{
    "id": "devopstest",
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "hub.docker.com/chess79/devopstest:latest",
            "network": "BRIDGE",
            "portMappings": [{ "hostPort": 80, "containerPort": 8080, "protocol": "tcp" }],
            "force_pull_image": true
        }
    },
    "cpus": 0.1,
    "instances": 1,
    "mem": 128,
    "acceptedResourceRoles": ["slave_public"]
}