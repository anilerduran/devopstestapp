{
    "id": "node.js devops test",
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "chess79/devopstest",
            "network": "BRIDGE",
            "portMappings": [
                { "hostport": 808, containerPort": 8080, "protocol": "tcp" }
            ]
        },
    },
    "cpus": 0.1,
    "instances": 1,
    "mem": 128,
    "acceptedResourceRoles": ["slave_public"]
}